alias tml='tmux list-sessions'
alias tma='tmux -2 attach -t $1'
alias tmk='tmux kill-session -t $1'
alias be='bundle exec'
alias cop_it='git ls-files -m | xargs ls -1 2>/dev/null | grep '\.rb$' | xargs rubocop -a'
alias c='coderay $1'
alias glog='git log --oneline --color | emojify | less -r'
alias gs='VISUAL=echo gem open $1'
alias ec2ssh='ssh -q -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no $1'
alias docker-compose-cp="function _f(){ docker cp \$2 \`docker-compose ps -q \$1\`:\$3;};_f"
